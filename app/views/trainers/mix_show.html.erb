<% provide(:title, 'Экзамен') %>

<%= form_tag check_mix_path, autocomplete:"off" do %>
<% @generated_tasks.each_with_index do |task,i| %>
        <%= label_tag 'mix_'+i.to_s,task[:text] %>
        <%= number_field_tag 'mix_'+i.to_s %>
        <br>
<% end %>
<%= submit_tag "Проверить все решения", class:"btn btn-large btn-primary"%>
<% end %>
